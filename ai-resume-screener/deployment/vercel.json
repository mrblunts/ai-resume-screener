{
  "version": 1,
  "name": "ai-resume-screener",
  "build": {
    "env": {
      "PYTHON_VERSION": "3.8"
    },
    "commands": {
      "install": "pip install -r requirements.txt",
      "build": "streamlit run src/ui/streamlit_app.py"
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/ui/streamlit_app.py"
    }
  ]
}